<Window xmlns="https://github.com/avaloniaui"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:vm="using:EamonPM.Game.ViewModels"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:views="clr-namespace:EamonPM.Game.Views"
		mc:Ignorable="d" 
		d:DesignWidth="600" 
		d:DesignHeight="800"
		Width="{Binding WindowWidth, Mode=TwoWay}"
		Height="{Binding WindowHeight, Mode=TwoWay}"
		WindowStartupLocation="CenterScreen"
		x:Class="EamonPM.Game.Views.MainWindow"
		x:Name="EamonPMMainWindow"
		Icon="avares://EamonPM/Assets/ten_sided_die.ico"
		Title="{Binding ProgramName, Mode=TwoWay}"
		SizeChanged="EamonPMMainWindow_SizeChanged"
		Closing="EamonPMMainWindow_Closed">
	<views:MainView />
</Window>
